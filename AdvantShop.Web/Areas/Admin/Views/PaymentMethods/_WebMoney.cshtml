@model AdvantShop.Web.Infrastructure.Admin.PaymentMethods.WebMoneyPaymentModel


    <div class="form-group row middle-xs">
        <div class="flex-grow-n col-fixed-size-md">
            <span class="text-required">WebMoney кошелёк магазина</span>
        </div>
        <div class="col-xs-6 relative">
            @Html.TextBoxFor(x => x.Purse, new { ng_model = "paymentMethod.Parameters.Purse", @class = "form-control", required = "required", validation_input_text = "WebMoney кошелёк магазина" })
        </div>
    </div>



    <div class="form-group row middle-xs">
        <div class="flex-grow-n col-fixed-size-md">
            <span class="text-required">Ключ для шифрования данных</span>
        </div>
        <div class="col-xs-6 relative">
            @Html.TextBoxFor(x => x.SecretKey, new { ng_model = "paymentMethod.Parameters.SecretKey", @class = "form-control", required = "required", validation_input_text = "Ключ для шифрования данных" })
        </div>
    </div>


